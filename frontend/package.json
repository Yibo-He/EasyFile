{
  "name": "EasyFile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "npm run serve"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.8",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
